{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@project:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_API_KEY": "@project:EXPO_PUBLIC_SUPABASE_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@project:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_API_KEY": "@project:EXPO_PUBLIC_SUPABASE_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "@project:EXPO_PUBLIC_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_API_KEY": "@project:EXPO_PUBLIC_SUPABASE_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
